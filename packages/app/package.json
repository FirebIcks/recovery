{
  "private": true,
  "name": "app",
  "version": "0.0.0",
  "main": "app/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "audit": "npx electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "dev": "nextron",
    "build": "nextron build",
    "build:all": "nextron build --all",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "build:mac": "nextron build --mac",
    "build:linux": "nextron build --linux",
    "lint": "next lint --fix renderer",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cross-spawn": "^7.0.3",
    "electron-log": "^4.4.8",
    "electron-serve": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.3.4",
    "portfinder": "^1.0.32",
    "secure-electron-context-menu": "^1.3.3",
    "secure-electron-store": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/prompt-cli": "^17.2.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.8",
    "@types/react": "^18.0.24",
    "electron": "21.2.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-turbo": "^0.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next": "^13.0.0",
    "nextron": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.4"
  }
}
