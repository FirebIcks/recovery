{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "$NODE_ENV"],
      "outputs": ["app/**", "renderer/.next/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": ["main/**", "renderer/**", "*.py"]
    },
    "dev": {
      "cache": false
    }
  }
}
