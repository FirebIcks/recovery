{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "build/**",
        "dist/**",
        "out/**",
        "app/**",
        "renderer/.next/**",
        "__main__.spec"
      ],
      "env": ["NODE_ENV", "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "format": {
      "outputs": [
        "main/**",
        "renderer/**",
        "components/**",
        "context/**",
        "lib/**",
        "pages/**",
        "*.py"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
