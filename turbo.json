{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "electron-builder.json",
        "app/**",
        "renderer/.next/**",
        "server/build/**",
        "server/dist/**",
        "server/__init__.spec"
      ],
      "env": ["NODE_ENV", "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "format": {
      "outputs": ["main/**", "renderer/**", "*.py"]
    },
    "dev": {
      "cache": false
    }
  }
}
